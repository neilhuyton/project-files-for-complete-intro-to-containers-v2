apiVersion: apps/v1
kind: Deployment
metadata:
  annotations:
    kompose.cmd: kompose convert --build local
    kompose.image-pull-policy: Always
    kompose.service.expose: "true"
    kompose.service.type: LoadBalancer
    kompose.version: 1.36.0 (ae2a39403)
