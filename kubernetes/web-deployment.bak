      labels:
        io.kompose.service: web
    spec:
      containers:
        # - image: web
        - image: us-central1-docker.pkg.dev/model-ripple-467310-c7/my-repo/web:latest
          # imagePullPolicy: Never
          imagePullPolicy: Always
          name: web
          ports:
            - containerPort: 80
              protocol: TCP
      restartPolicy: Always
